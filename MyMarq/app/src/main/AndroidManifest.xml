<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mymarq">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EngineeringHall"
            android:parentActivityName=".MainActivity">

            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".Ehall_F1"
            android:parentActivityName=".EngineeringHall">

            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".EngineeringHall" />
        </activity>
        <activity android:name=".Button_Activity"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".amu_floors"></activity>
        <activity android:name=".cudahy_floors"></activity>
        <activity android:name=".haggerty_floors"></activity>
        <activity android:name=".marquette_floors"></activity>
        <activity android:name=".memorial_floors"></activity>
        <activity android:name=".olin_floors"></activity>
        <activity android:name=".raynor_floors"></activity>
    </application>

</manifest>